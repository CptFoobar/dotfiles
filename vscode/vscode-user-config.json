            "backgroundColor": "none",
            "overviewRulerColor": "#63c662",
            "isWholeLine": false
        }
    ],
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss",
        "**/*.go"
    ],
    "todohighlight.exclude": [
        "**/vendor/**",
        "**/node_modules/*"
    ],
    "workbench.statusBar.feedback.visible": false,

    "workbench.iconTheme": "material-icon-theme",
    "debug.toolBarLocation": "docked",
    "editor.insertSpaces": false,
    "go.enableCodeLens": {
        "references": false,
        "runtest": true
    },
    "docthis.authorName": "Sidhant Sharma",
    "doxdocgen.generic.authorTag": "Sidhant Sharma",
    "doxdocgen.generic.dateFormat": "YYYY/MM/DD",
    "doxdocgen.generic.briefTemplate": "@fileoverview {text}",
    "breadcrumbs.enabled": true,
    "editor.fontSize": 14,
    "workbench.colorTheme": "Visual Studio Dark",
    "nodeTdd.buildOnActivation": true,
    "nodeTdd.buildOnDelete": true,
    "nodeTdd.buildOnCreate": true,
    "nodeTdd.glob": "{src,test,app}/**/*.{js,ts,jsx,tsx,spec.js}",
    "mocha.files.glob": "{test,app}/**/*.js",
    "python.pythonPath": "D:\\Continuum\\Anaconda3\\envs\\py3\\python.exe",
    "gitlens.gitExplorer.files.layout": "tree",
    "gitlens.views.repositories.files.layout": "tree",
    "C_Cpp.clang_format_fallbackStyle": "{BasedOnStyle: LLVM, IndentWidth: 4}",
    "python.jediEnabled": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "[typescript]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": false,
        "editor.detectIndentation": false
    },
    "[html]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    }
}
