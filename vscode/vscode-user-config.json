        },
        {
            "text": "TODO:",
            "color": "#dab900",
            "border": "1px solid #dab900",
            "backgroundColor": "none",
            "overviewRulerColor": "#dab900",
            "isWholeLine": false
        },
        {
            "text": "FIXME:",
            "color": "pink",
            "border": "1px solid #c800f0",
            "backgroundColor": "none",
            "overviewRulerColor": "#c800f0",
